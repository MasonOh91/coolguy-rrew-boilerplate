{
  "name": "coolguy-rrew-boilerplate",
  "version": "0.1.0",
  "description": "A boilerplate for running a Webpack workflow in Node express",
  "main": "server.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/MasonOh91/coolguy-rrew-boilerplate.git"
  },
  "keywords": [
    "express",
    "webpack",
    "redux",
    "react",
    "node"
  ],
  "author": "Mason O'Mara/Christian Alfoni",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MasonOh91/coolguy-rrew-boilerplate/issues"
  },
  "homepage": "https://github.com/MasonOh91/coolguy-rrew-boilerplate",
  "engines": {
    "node": "4.2.6"
  },
  "scripts": {
    "start": "node server.js",
    "build": "rimraf dist && set NODE_ENV=production && webpack --config ./webpack.production.config.js --progress --profile --colors",
    "clean-production": "rimraf app && rm -f .babelrc && rm -f .eslintignore && rm -f .eslintrc && rm -f .eslint && rm -f .jsbeautifyrc && rm -f .jscsrc && rm -f webpack.config.js && rm -f webpack.production.config.js", 
    "eslint": "eslint .",
    "jscs": "jscs ."
  },
  "dependencies": {
    "express": "^4.13.3",
    "jsdom": "^6.5.1",
    "react": "^0.14.3",
    "react-addons-test-utils": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-redux": "^4.4.0",
    "redux": "^3.3.1",
    "webpack": "^1.12.2",
    "rimraf": "^2.4.3"
  },
  "devDependencies": {
    "autoprefixer": "^6.0.3",
    "babel-cli": "^6.4.0",
    "babel-core": "^6.3.26",
    "babel-eslint": "^4.1.6",
    "babel-jscs": "^2.0.5",
    "babel-loader": "^6.2.1",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "react-transform-hmr": "^1.0.0",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.12.4",
    "url-loader": "^0.5.7",
    "static-loader": "^0.1.8",
    "json-loader": "^0.5.3",
    "node-sass": "^3.4.2",
    "css-loader": "^0.19.0",
    "eslint": "^1.5.0",
    "eslint-plugin-react": "^3.4.2",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.2.0",
    "react-hot-loader": "^1.3.0",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^1.6.1",
    "normalize.css": "^3.0.3",
    "jscs": "^2.1.1"
  }
}
